                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>CP1252</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>CP1253</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>CP1254</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>CP1255</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>CP1256</string>
                        </property>
                    </item>
                    <item>
                        <property name="text">
                            <string>CP1257</string>
                        </property>
                    </item>
