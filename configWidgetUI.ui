                    <property name="name">
                        <cstring>textLabel1</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;LCD color:</string>
                    </property>
                    <property name="buddy" stdset="0">
                        <cstring>kcfg_ledColor</cstring>
